state_machine

states s1, s2, s3;

final states s4;

inputs a, b, c;

outputs sal;

starts with s1;

sal:='0';

begin

transition(s1,a,s2);

transition(s2,c,s1);

transition(s2,b,s3);

transition(s3, a, s2);

transition(s3, b, s1);

transition(s3, c, s4);

behaviour
sal:='1';

end
